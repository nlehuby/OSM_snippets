{
    "schema": "iglu:com.snowplowanalytics.factotum/factfile/jsonschema/1-0-0",
    "data": {
        "name": "get all lines of idf",
        "tasks": [{
            "name": "download",
            "executor": "shell",
            "command": "wget http://download.geofabrik.de/europe/france/ile-de-france-latest.osm.pbf --no-verbose 2>&1",
            "arguments": [],
            "dependsOn": [],
            "onResult": {
                "terminateJobWithSuccess": [],
                "continueJob": [0]
            }
        }, {
            "name": "rename",
            "executor": "shell",
            "command": "mv ile-de-france-latest.osm.pbf fr-idf.osm.pbf",
            "arguments": [],
            "dependsOn": ["download"],
            "onResult": {
                "terminateJobWithSuccess": [],
                "continueJob": [0]
            }
        }, {
            "name": "extract route master",
            "executor": "shell",
            "command": "osmosis --read-pbf fr-idf.osm.pbf --tf accept-relations type=route_master --used-way --used-node --write-pbf lignes.osm.pbf 2>&1",
            "arguments": [],
            "dependsOn": ["rename"],
            "onResult": {
                "terminateJobWithSuccess": [],
                "continueJob": [0]
            }
        }, {
            "name": "csv",
            "executor": "shell",
            "command": "osmconvert64 lignes.osm.pbf --csv='@id ref name network operator colour type route_master ref:FR:STIF:ExternalCode_Line' --csv-headline > lignes_tofilter.csv",
            "arguments": [],
            "dependsOn": ["extract route master"],
            "onResult": {
                "terminateJobWithSuccess": [],
                "continueJob": [0]
            }
        }, {
            "name": "filter csv",
            "executor": "shell",
            "command": "cat lignes_tofilter.csv |xsv search -s type route_master -d '\t' > lignes_.csv",
            "arguments": [],
            "dependsOn": ["csv"],
            "onResult": {
                "terminateJobWithSuccess": [],
                "continueJob": [0]
            }
        }, {
            "name": "remove national trains",
            "executor": "shell",
            "command": "cat lignes_.csv |xsv search -s network TGV -v | xsv search -s network Eurostar -v |xsv search -s network Thalys -v |xsv search -s network Flixbus -v |xsv search -s network Intercités -v|xsv search -s operator 'SNCF Réseau' -v |xsv search -s operator 'DB Fernverkehr' -v > lignes.csv",
            "arguments": [],
            "dependsOn": ["filter csv"],
            "onResult": {
                "terminateJobWithSuccess": [],
                "continueJob": [0]
            }
        }, {
            "name": "remove bicycle and ferry",
            "executor": "shell",
            "command": "mv lignes.csv lignes_.csv && cat lignes_.csv |xsv search -s route_master bicycle -v | xsv search -s route_master ferry -v  > lignes.csv",
            "arguments": [],
            "dependsOn": ["remove national trains"],
            "onResult": {
                "terminateJobWithSuccess": [],
                "continueJob": [0]
            }
        }, {
            "name": "clean temp pbf",
            "executor": "shell",
            "command": "rm *.osm.pbf",
            "arguments": [],
            "dependsOn": ["remove bicycle and ferry"],
            "onResult": {
                "terminateJobWithSuccess": [],
                "continueJob": [0]
            }
        }, {
            "name": "clean temp csv",
            "executor": "shell",
            "command": "rm lignes_*.csv",
            "arguments": [],
            "dependsOn": ["remove bicycle and ferry"],
            "onResult": {
                "terminateJobWithSuccess": [],
                "continueJob": [0]
            }
        }]
    }
}
